    #fh5co-counter.fh5co-bg.fh5co-counter(style='background-image:url(images/img_bg_5.jpg);')
      .container
        .row
          .display-t
            .display-tc
              .col-md-3.col-sm-6.animate-box
                .feature-center
                  span.icon
                    i.icon-eye
                  span.counter.js-counter(data-from='0' data-to='22070' data-speed='5000' data-refresh-interval='50') 1
                  span.counter-label Creativity Fuel
              .col-md-3.col-sm-6.animate-box
                .feature-center
                  span.icon
                    i.icon-shopping-cart
                  span.counter.js-counter(data-from='0' data-to='450' data-speed='5000' data-refresh-interval='50') 1
                  span.counter-label Happy Clients
              .col-md-3.col-sm-6.animate-box
                .feature-center
                  span.icon
                    i.icon-shop
                  span.counter.js-counter(data-from='0' data-to='700' data-speed='5000' data-refresh-interval='50') 1
                  span.counter-label All Products
              .col-md-3.col-sm-6.animate-box
                .feature-center
                  span.icon
                    i.icon-clock
                  span.counter.js-counter(data-from='0' data-to='5605' data-speed='5000' data-refresh-interval='50') 1
                  span.counter-label Hours Spent
    #fh5co-started
      .container
        .row.animate-box
          .col-md-8.col-md-offset-2.text-center.fh5co-heading
            h2 Newsletter
            p Just stay tune for our latest Product. Now you can subscribe
        .row.animate-box
          .col-md-8.col-md-offset-2
            form.form-inline
              .col-md-6.col-sm-6
                .form-group
                  label.sr-only(for='email') Email
                  input#email.form-control(type='email' placeholder='Email')
              .col-md-6.col-sm-6
                button.btn.btn-default.btn-block(type='submit') Subscribe
